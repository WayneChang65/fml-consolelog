{
  "name": "@waynechang65/fml-consolelog",
  "version": "1.2.3",
  "description": "fml-consolelog is a formatted multi-line and ANSI colors supportted text output function module.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "directories": {
    "lib": "dist",
    "example": "src/examples",
    "test": "src/test"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "start": "tsx src/examples/demo.ts",
    "start-cjs": "node src/examples/demo.cjs",
    "publish": "npm run build && npm publish"
  },
  "keywords": [
    "console-log",
    "cli",
    "console",
    "text"
  ],
  "author": "WayneChang65",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WayneChang65/fml-consolelog/issues"
  },
  "homepage": "https://github.com/WayneChang65/fml-consolelog#readme",
  "dependencies": {
    "ascii-string-align": "https://github.com/WayneChang65/ascii-string-align/tarball/v1.1.0a",
    "chalk": "^5.4.1",
    "string-width": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "vitest": {
      "rollup": "npm:@rollup/wasm-node"
    }
  }
}